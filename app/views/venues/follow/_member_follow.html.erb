<%= link_to "Review", new_users_review_path(:reviewable_id => @reviewable.id, :reviewable_type => params["controller"]), class:"waves-effect waves-light btn-large" %>
<% if current_user.followed(@venue) %>
  <%= link_to "Unfollow #{@venue.class}", user_follow_path(current_user, current_user.followed(@venue)), :method => :delete, class:"waves-effect waves-light btn-large" %>
<% else %>
  <% follow_params = {user_id: current_user.id, target_id: @venue.id, target_type: @venue.class} %>

  <%= link_to "Follow #{@venue.class}", user_follows_path(current_user, follow_params), :method => :post, class:"waves-effect waves-light btn-large" %>
<% end %>
