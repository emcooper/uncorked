<%= link_to "Review", new_users_review_path(:reviewable_id => @reviewable.id, :reviewable_type => params["controller"]) %>
<% if current_user.is_following?(@venue) %>
  <%= link_to "Unfollow #{@venue.class}", follow_path(current_user.is_following?(@venue)), :method => :delete %>
<% else %>
  <% follow_params = {user_id: current_user.id, target_id: @venue.id, target_type: @venue.class} %>
  <%= link_to "Follow #{@venue.class}", follows_path(follow_params), :method => :post %>
<% end %>
